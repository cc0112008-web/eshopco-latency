{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/latency-metrics",
      "dest": "main.py",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/health",
      "dest": "main.py",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "main.py",
      "methods": ["GET"]
    }
  ]
}
